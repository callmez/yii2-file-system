    "license": "MIT",
    "support": {
        "issues": "https://github.com/callmez/yii2-file-system/issues",
        "wiki": "https://github.com/callmez/yii2-file-system/wiki",
        "source": "https://github.com/callmez/yii2-file-system"
    },
    "authors": [
        {
            "name": "CallMeZ",
            "email": "callme-z@qq.com"
        }
    ],
    "require": { 
        "yiisoft/yii2": "*",
        "qiniu/php-sdk": "~6.0",
        "league/flysystem": "*"
    },
    "autoload": {
        "psr-4": {
            "callmez\\file\\system\\": ""
        }
    }
